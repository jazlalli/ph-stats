<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <h2 class="sub-header">Weekly Data</h2>
  <div class="table-responsive">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>#</th>
          <th>Avg No. of Votes</th>
          <th>Day of the week</th>
        </tr>
      </thead>
      <tbody>
      	{{#each data.dayofweek }}
					<tr>
						<td>{{@index}}</td>
						<td>{{this.avg}}</td>
						<td>{{this.dayofweek}}</td>
					</tr>
				{{/each}}
      </tbody>
    </table>
  </div>
</div>

